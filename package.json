{
  "name": "protractor-starter",
  "version": "0.0.4",
  "description": "protractor-starter end-to-end tests setup also for non angular application",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/marcelmokos/protractor-starter.git",
    "type": "git"
  },
  "author": "Marcel Moko≈°",
  "license": "MIT",
  "scripts": {
    "start": "yarn run test",
    "test": "yarn install && yarn lint:fix && yarn run protractor",
    "test:multiple": "yarn install && yarn lint:fix && yarn run protractor -- --capabilities.shardTestFiles=true --capabilities.maxInstances=3",
    "protractor": "babel-node node_modules/protractor/bin/protractor protractor-conf.js",
    "lint:fix": "eslint \"spec/**\" --fix"
  },
  "pre-commit": [
    "lint:fix"
  ],
  "dependencies": {
    "babel": "latest",
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-stage-0": "latest",
    "chromedriver": "latest",
    "jasmine-spec-reporter": "latest",
    "node-fetch": "latest",
    "protractor": "latest"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-protractor": "latest",
    "pre-commit": "latest"
  }
}
