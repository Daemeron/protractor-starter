{
  "name": "protractor-starter",
  "version": "0.0.1",
  "description": "protractor end-to-end tests for non angular application",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/marcelmokos/protractor-starter.git",
    "type": "git"
  },
  "author": "Marcel Moko≈°",
  "license": "MIT",
  "scripts": {
    "start": "yarn run test",
