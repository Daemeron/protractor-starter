{
  "name": "protractor-starter",
  "version": "0.0.5",
  "description": "protractor-starter end-to-end tests setup also for non angular application",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/marcelmokos/protractor-starter.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/marcelmokos/protractor-starter/issues"
  },
  "keywords": [
    "protractor",
    "starter",
    "page object pattern",
    "testing",
    "end to end testing"
  ],
  "author": "Marcel Moko≈°",
  "license": "MIT",
  "scripts": {
    "start": "yarn run test",
    "pretest": "yarn install && yarn run lint:fix",
    "test": "yarn pretest && yarn run protractor",
    "test:multiple": "yarn pretest && yarn run protractor -- --capabilities.shardTestFiles=true --capabilities.maxInstances=3",
    "protractor": "babel-node node_modules/.bin/protractor protractor-conf.js",
    "lint": "eslint \"spec/**\" --cache",
    "lint:fix": "yarn run lint -- --fix",
    "lint:pre-comit": "yarn run lint -- --fix --rule 'no-console:2' --rule 'no-debugger:2' --rule 'protractor/no-browser-pause:2'"
  },
  "pre-commit": [
    "lint:pre-comit"
  ],
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chromedriver": "^2.26.1",
    "jasmine-spec-reporter": "^3.0.0",
    "node-fetch": "^1.6.3",
    "protractor": "^4.0.14"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-protractor": "^1.32.0",
    "pre-commit": "^1.2.2"
  }
}
